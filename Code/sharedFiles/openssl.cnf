[ req ]
default_bits        = 2048
default_md          = sha256
default_keyfile     = key.pem
prompt              = no
encrypt_key         = no
distinguished_name  = req_distinguished_name
x509_extensions     = v3_req

[ req_distinguished_name ]
C            = GB
ST           = Fife
L            = St Andrews
O            = University of St Andrews
CN           = 127.0.0.1

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1   = localhost
IP.1    = 127.0.0.1
